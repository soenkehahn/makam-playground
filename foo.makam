tests : testsuite.

%testsuite tests.

mySum : list (list int) -> int -> prop.
mySum List Result <-
  foldl
    (pfun Acc InnerList Res => [InnerListSum]
      print InnerList,
      foldl plus 0 InnerList InnerListSum,
      print InnerListSum,
      plus Acc InnerListSum Res)
    0 List Result.

test_folds : int -> prop.
test_folds A <-
  mySum [[1], [2]] A.
test_folds X?
>> Yes:
>> X := 3.
